void M_set_DC(unsigned char DutyCycle, unsigned char motor) //1:CCP1 - 2:CCP2 - 3:CCP1&CCP2, Assuming PR2=0xFF
{
    if(motor<1||motor>3||DutyCycle<=0||DutyCycle>100)return;
    switch(motor)
    {
        case 1:
            if(DutyCycle==10)
            {
                CCPR1L=0b00011001;
                DC1B1=1;        
                DC1B0=0;
            }
            if(DutyCycle==20)
            {
                CCPR1L=0b00110011;
                DC1B1=0;        
                DC1B0=0;
            }
            if(DutyCycle==30)
            {
                CCPR1L=0b01001100;
                DC1B1=1;        
                DC1B0=1;
            }
            if(DutyCycle==40)
            {
                CCPR1L=0b01100110;
                DC1B1=0;        
                DC1B0=1;
            }
            if(DutyCycle==50)
            {
                CCPR1L=0b10000000;
                DC1B1=0;        
                DC1B0=0;
            }
            if(DutyCycle==60)
            {
                CCPR1L=0b10011001;
                DC1B1=1;        
                DC1B0=0;
            }
            if(DutyCycle==70)
            {
                CCPR1L=0b10110011;
                DC1B1=0;        
                DC1B0=0;
            }
            if(DutyCycle==80)
            {
                CCPR1L=0b11001100;
                DC1B1=1;        
                DC1B0=0;
            }
            if(DutyCycle==90)
            {
                CCPR1L=0b11100110;
                DC1B1=0;        
                DC1B0=1;

            }
            if(DutyCycle==100)
            {
                CCPR1L=0xFF;
                DC1B1=1;        
                DC1B0=1;
            }
            break;
        case 2:
            if(DutyCycle==10)
            {
                CCPR2L=0b00011001;
                DC2B1=1;        
                DC2B0=0;
            }
            if(DutyCycle==20)
            {
                CCPR2L=0b00110011;
                DC2B1=0;        
                DC2B0=0;
            }
            if(DutyCycle==30)
            {
                CCPR2L=0b01001100;
                DC2B1=1;        
                DC2B0=1;
            }
            if(DutyCycle==40)
            {
                CCPR2L=0b01100110;
                DC2B1=0;        
                DC2B0=1;
            }
            if(DutyCycle==50)
            {
                CCPR2L=0b10000000;
                DC2B1=0;        
                DC2B0=0;
            }
            if(DutyCycle==60)
            {
                CCPR2L=0b10011001;
                DC2B1=1;        
                DC2B0=0;
            }
            if(DutyCycle==70)
            {
                CCPR2L=0b10110011;
                DC2B1=0;        
                DC2B0=0;
            }
            if(DutyCycle==80)
            {
                CCPR2L=0b11001100;
                DC2B1=1;        
                DC2B0=0;
            }
            if(DutyCycle==90)
            {
                CCPR2L=0b11100110;
                DC2B1=0;        
                DC2B0=1;
            }
            if(DutyCycle==100)
            {
                CCPR2L=0xFF;
                DC2B1=1;        
                DC2B0=1;
            }
            break;
        case 3:
            if(DutyCycle==10)
            {
                CCPR1L=0b00011001;
                DC1B1=1;        
                DC1B0=0;
                CCPR2L=0b00011001;
                DC2B1=1;        
                DC2B0=0;
            }
            if(DutyCycle==20)
            {
                CCPR1L=0b00110011;
                DC1B1=0;        
                DC1B0=0;
                CCPR2L=0b00110011;
                DC2B1=0;        
                DC2B0=0;
            }
            if(DutyCycle==30)
            {
                CCPR1L=0b01001100;
                DC1B1=1;        
                DC1B0=1;
                CCPR2L=0b01001100;
                DC2B1=1;        
                DC2B0=1;
            }
            if(DutyCycle==40)
            {
                CCPR1L=0b01100110;
                DC1B1=0;        
                DC1B0=1;
                CCPR2L=0b01100110;
                DC2B1=0;        
                DC2B0=1;
            }
            if(DutyCycle==50)
            {
                CCPR1L=0b10000000;
                DC1B1=0;        
                DC1B0=0;
                CCPR2L=0b10000000;
                DC2B1=0;        
                DC2B0=0;
            }
            if(DutyCycle==60)
            {
                CCPR1L=0b10011001;
                DC1B1=1;        
                DC1B0=0;
                CCPR2L=0b10011001;
                DC2B1=1;
                DC2B0=0;
            }
            if(DutyCycle==70)
            {
                CCPR1L=0b10110011;
                DC1B1=0;        
                DC1B0=0;
                CCPR2L=0b10110011;
                DC2B1=0;        
                DC2B0=0;
            }
            if(DutyCycle==80)
            {
                CCPR1L=0b11001100;
                DC1B1=1;        
                DC1B0=0;
                CCPR2L=0b11001100;
                DC2B1=1;        
                DC2B0=0;
            }
            if(DutyCycle==90)
            {
                CCPR1L=0b11100110;
                DC1B1=0;        
                DC1B0=1;
                CCPR2L=0b11100110;
                DC2B1=0;        
                DC2B0=1;
            }
            if(DutyCycle==100)
            {
                CCPR1L=0xFF;
                DC1B1=1;        
                DC1B0=1;
                CCPR2L=0xFF;
                DC2B1=1;        
                DC2B0=1;
            }
            break;
    }
}
